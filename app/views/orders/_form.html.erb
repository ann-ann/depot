<%= form_for(@order) do |f| %>

<%= render "shared/error_messages", :target => f.object %>

  <div class="field">
    <%= f.label :bill_address %><br>
    <%= f.text_field :bill_address %>
  </div>
  <div class="field">
    <%= f.label :ship_address %><br>
    <%= f.text_field :ship_address %>
  </div>


  <div class="actions">
     <%= submit_tag("Done!") %>
  </div>
<% end %>
